<script>
    aj = new XMLHttpRequest()
    function typedef() {
        afel = document.getElementById( "add_form" ).elements
        utype = afel.utype.value

        aj.onreadystatechange = () => {
            if( aj.readyState == 4 ) {
                res = aj.responseText
                res = res.split( ";" )

                afel.brand.innerHTML = ""
                res.forEach( cur => {
                    cur = cur.split( ", " )
                    afel.brand.innerHTML += "<option value='" + cur[0] + "'>" + cur[1] + "</option>"
                })                
            }
        }

        aj.open( "GET", "/admin/maintenance/unit/typedef?utype=" + utype )
        aj.send()
    }

    function process_text(){
        f = document.getElementById("add_form").elements
        bn = f.bodyno.value
        if ( bn.length == 17 ){
            f.chassino.value = bn.substr (11, 16)
        }
    }
</script>
<script>
    ( function () {
        upr = document.getElementById( "add_form" ).elements.upregistration
        rr = document.getElementById( "add_form" ).elements.rreg
        d = new Date()
        year = String( d.getFullYear() )
        month = d.getMonth() + 1
        day = d.getDate()

        n = year + "-" + ( month < 9? "0" + month: month ) + "-" + ( day < 9? "0" + day: day )
        upr.setAttribute( "max", n )
        rr.setAttribute( "max", n )
    } )()

    <% if( Object.keys( fform ).length > 1 ) { %>
            document.getElementById( "addbutton" ).click()
            swal({ title: "Warning!",
                   text: "<%= fdesc %>",
                   timer: 2000,
                   type: 'warning',
                   showConfirmButton: false
                }).catch(swal.noop)
    <% } else { %>
            <% if( added == 1 ) { %>
                swal({
                    title: "Unit Successfully Added!",
                    // text: "added!",
                    buttonsStyling: false,
                    confirmButtonClass: "btn btn-success",
                    type: "success"
                }).catch(swal.noop)
            <% } else if( deleted == 1 ) { %>
                swal({
                    title: "Unit Successfully Deleted!",
                    // text: "added!",
                    buttonsStyling: false,
                    confirmButtonClass: "btn btn-success",
                    type: "success"
                }).catch(swal.noop)
            <% } else if( deleted == 2 ) { %>
                swal({ title: "Warning!",
                   text: "<%= fdesc %>",
                   timer: 2000,
                   type: 'warning',
                   showConfirmButton: false
                }).catch(swal.noop)
            
                <% } %>
                <% } %>

            function del_rec( par )
            {
                swal({
                    title: 'Are you sure?',
                    text: "You won't be able to revert this!",
                    type: 'warning',
                    showCancelButton: true,
                    confirmButtonClass: 'btn btn-success' ,
                    cancelButtonClass: 'btn btn-danger',
                    confirmButtonText: 'Yes, delete it!',
                    buttonsStyling: false
                }).then(function() {
                window.location = "/admin/maintenance/unit/delete/" + par
                }).catch(swal.noop);

            }
</script>

<script>
    $(document).ready(function(){
        var $this = $(this)
        $('#maintenance').addClass('active');
        if($this.hasClass('show')) {
            $('#maintenance.collapse').slideDown('slow','swing').removeClass('show')
        }
        else{
            $('#maintenance.collapse').slideUp('slow','swing').addClass('show')
        }
        $('#pagesMaintenance.collapse').addClass('show');
        
        $('#unit').addClass('active');
    })
</script>